import java.util.Scanner;

public class Main 
{
	void input()
	{
		Scanner sc = new Scanner(System.in);
		
		System.out.println(" Enter the name of the course \n\n");
		
		System.out.println(" Enter the date of the booking \n\n");
		
		System.out.println(" Enter the number of people that will be attending \n\n");
		
		System.out.println(" Enter the duration (whole numbers, hours) \n\n");
	}
	
	Main()
	{
		input();
	}
	
	public static void main(String[] args) 
	{
		Main theMain = new Main();
	}

}
